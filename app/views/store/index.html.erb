<h1>Stores</h1>

  <h3> <%= @all_stores.count %> Total Stores</h3>
<ul class="stores">
<% @stores.each do |store| %>
    <li id="sore_list"><%= store.long_name %></li>
    <li id="sore_list"><%= store.city %></li>
    <li id="sore_list"><%= store.distance %></li>
    <li id="sore_list"><%= store.phone_number %></li>
    <li id="sore_list"><%= store.store_type %></li>
<% end %>
</ul>
